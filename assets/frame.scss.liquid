.about-us .img-section-desc.left-section{float: right; margin-right: -60px;position: relative;@include at-query($max, $medium) { margin-right:0;} }

.about-us .section-seven {float: left;width: 100%;padding: 5% 0;}

.about-us .newsletter-section {text-align: center;float:left;width:100%;}

.about-us .news-letterform {@include at-query($max, $small){width:100%;margin-bottom:20px;}float: left;position: relative;width: 60%;}

.about-us .news-letterform form {margin: 0;display: inline-block;width: 80%;float:none;}

.about-us .newsletter-section form input[type="email"] {@include at-query($max, $postlarge){width: 78%;}@include at-query($max, $semilarge){width: 70%;}@include at-query($max, $small){width:100%;}border: none;border-radius: 0;width: 83%;}

.about-us .news-letterform input[type="email"] {float: left;height: 50px;}

.about-us .news-letterform button {@include at-query ($max, $postlarge){margin-left: -8px}display: inline-block;margin-bottom: 0;margin-left: -4px;padding: 15px 30px;border:none;}

.about-us .news-info {@include at-query($max, $small){width:100%;}float: left;width: 20%;padding: 10px 20px;border-radius: 7px;position: relative;}

.about-us .news-info::after {border-left: 20px solid transparent;content: "";height: 0;left: -20px;position: absolute;top: 15px;width: 0;}

.about-us .grid-uniform.section-seven {float: left;width: 100%;}

.about-us .news-info p {margin: 0;}

.about-us .main-heading {float: left;text-align: center;width: 100%;margin-bottom: 40px;}

.about-us .section-seven .main-title { float: left;text-align: center; width: 100%;}

