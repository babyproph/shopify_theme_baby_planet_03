.about-us .section-four {float: left;padding: 5% 0;width: 100%;}

.about-us .section-four  .inner-row{margin-bottom:60px;}

.about-us .img-section-desc.right-section {margin-left: -80px;@include at-query($max, $medium) {margin-left: 0px; } }

.about-us .img-section-desc {@include at-query($max, $postlarge){margin-top: 40px;}margin-top: 60px;padding: 40px;width: 430px;@include at-query($max, $semilarge) { width:400px;margin-top:15px; }@include at-query($max, $medium) { width:100%;pading:20px;margin-top:20px; }}

