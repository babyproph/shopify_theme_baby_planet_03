.about-us .client-section .item a:hover{border:none;}

.about-us .section-four .icon::after {content: "";height: 100%;left: 0;position: absolute;width:0%;z-index: -1;-webkit-transition:all 0.3s linear 0s;transition: all 0.3s linear 0s;}

.about-us .section-four .icon{z-index: 1;}

.about-us .section-four .icon-wrapper:hover .icon::after{width:100%;}

.about-us .section-four .icon-wrapper  h4{@include at-query($max, $postlarge){font-size: 20px;}}




 /* Img Carousel Type */  

.img-carousel-type .left-img{@include at-query($max, $semilarge){padding-right:0px;}padding-right: 20px;}
.img-carousel-type .img-content h3 {font-size: 30px;position: relative;padding-bottom: 15px;margin-bottom: 30px;}
.img-carousel-type .img-content h3::before { position: absolute;height: 1px;width: 45px;content: "";bottom: 0;left: 0;}
.img-carousel-type .img-wrapper{overflow:hidden;padding: 30px 60px;width: 100%;float: left;position:relative;}
.img-carousel-type p{margin-bottom:10px;}
.img-carousel-type .img-block {padding: 0 10px;}
.img-carousel-type .img-content {@include at-query($max, $semilarge){padding: 20px 30px;}@include at-query($max, $postlarge){padding: 25px 25px;}padding: 40px 60px;}
.img-carousel-type .nav_article {position: absolute;width: 100%;top: 50%;}
.img-carousel-type .nav_article .prev {position: absolute;left: -45px;text-align:center;margin:auto;}
.img-carousel-type .nav_article .next {position: absolute;right: 73px;text-align:center;margin:auto;}
.img-carousel-type .nav_article a { height: 35px;width: 35px;padding: 0;line-height: 32px;font-size:20px;border-radius: 50%;-ms-transform: translateY(-50%); -webkit-transform: translateY(-50%);transform: translateY(-50%);position: absolute;}
  



 /*Border Icon Section */
  
.border-icon-section .icon_section { box-shadow: 0 1.063rem 2.188rem rgba({{ settings.black_borders }},0.03); @include transition(all 0.5s ease-in-out);display:inline-block;width:100%;text-align:center;position:relative;padding: 30px 30px 40px 30px; }
.border-icon-section .icon_block { list-style:none;padding:0; }
.border-icon-section .icon_block li { margin-bottom:0px;padding:0 15px;@include at-query($max,$semilarge){margin-bottom:60px;}@include at-query($max,$small){margin-bottom:60px;} }
.border-icon-section .icon_block ul { margin-left:-20px; @include at-query($max,$small) { margin-left:0; }}
.border-icon-section h2 {}
.border-icon-section .icon_text p {font-size: 14px;  letter-spacing:0;margin-bottom: 40px;}
.border-icon-section .icon_block img{@include at-query($max,$postlarge){height:200px;width:200px;}border-radius: 50%;@include transition(all 0.5s ease-in-out);height:300px;width:300px;}  
.border-icon-section .icon_section:hover img {  }
.border-icon-section .icon_section .icon_img{margin-bottom: 20px;overflow:hidden;}
.border-icon-section .icon_section::after {position: absolute;content: "";height: 12px;width: 12px;left: 0;right: 0;margin: 0 auto; bottom: -6px;@include transition(all 0.5s ease-in-out);transform:rotate(-45deg);-webkit-transform:rotate(-45deg);-moz-transform:rotate(-45deg);-ms-transform:rotate(-45deg);-o-transform:rotate(-45deg);transform:rotate(-45deg);}
.border-icon-section .icon_block .btn {text-transform: inherit;padding: 10px 20px;font-size: 12px;}  
    
